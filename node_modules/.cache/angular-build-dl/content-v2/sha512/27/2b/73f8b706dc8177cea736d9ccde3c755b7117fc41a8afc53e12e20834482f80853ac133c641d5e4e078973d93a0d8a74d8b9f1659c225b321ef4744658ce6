                                style: {
                                    color: 'white',
                                    textOutline: 'transparent'
                                }
                            },
                            data: [{
                                    name: 'Budget',
                                    y: parseInt(res.result.result.achieved.orderBudget),
                                    drilldown: ''
                                }, {
                                    name: 'Pipeline',
                                    y: 0,
                                    drilldown: ''
                                }, {
                                    name: 'Estimate',
                                    y: 0,
                                    drilldown: ''
                                }]
                        }, {
