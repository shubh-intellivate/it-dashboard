    createOrderAmtGraph(bu, start_date, end_date, geo, currency, timeframe) {
        console.log("here");
        let data = {
            "bu": bu,
            "start_date": start_date,
            "end_date": end_date,
            "geo": geo,
            "currency": currency,
            "fiscal_year": "",
            "timeframe": timeframe
        };
        this.dataService.getOrderConversionAmt(data).subscribe(res => {
            if (res.result.status == "true") {
                this.orderAmtPerValue = res.result.result.percentage + '%';
                const chart_line_top_accounts = highcharts__WEBPACK_IMPORTED_MODULE_2__["chart"]('chart-order-amt', {
                    chart: {
                        zoomType: 'xy'
                    },
                    title: {
                        text: ''
                    },
                    colors: ['rgb(70,121,167)', 'rgb(162,197,238)'],
                    yAxis: {
                        title: {
                            text: ''
                        },
                        gridLineColor: 'transparent',
                        visible: false
                    },
                    xAxis: {
                        categories: ['PY', 'Q1', 'Q2', 'Q3', 'Q4']
                    },
                    plotOptions: {
                        line: {
                            dataLabels: {
                                enabled: true
                            }
                        },
                        column: {
                            dataLabels: {
                                enabled: true,
                                formatter: function () {
                                    return this.y + '%';
                                }
                            }
                        },
                    },
                    tooltip: {
                        pointFormat: '<span style="color:{series.color}">{series.name}: {point.y}%</span><br/>'
                    },
                    series: [{
                            name: 'Percentage',
                            type: 'column',
                            showInLegend: false,
                            data: [parseFloat(res.result.result.drilldown.prev_year.Q1), parseFloat(res.result.result.drilldown.Q1), parseFloat(res.result.result.drilldown.Q2), parseFloat(res.result.result.drilldown.Q3), parseFloat(res.result.result.drilldown.Q4)]
                        }, {
                            name: 'Percentage',
                            type: 'spline',
                            showInLegend: false,
                            data: [parseFloat(res.result.result.drilldown.prev_year.Q1), parseFloat(res.result.result.drilldown.Q1), parseFloat(res.result.result.drilldown.Q2), parseFloat(res.result.result.drilldown.Q3), parseFloat(res.result.result.drilldown.Q4)]
                        }],
                    responsive: {
                        rules: [{
                                condition: {
                                    maxWidth: 500
                                },
                                chartOptions: {
                                    legend: {
                                        layout: 'horizontal',
                                        align: 'center',
                                        verticalAlign: 'bottom'
                                    }
                                }
                            }]
                    }
                });
            }
        });
    }
